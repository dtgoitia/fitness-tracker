{
  "name": "fitness-tracker",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://davidtorralba.com/fitness-tracker",
  "dependencies": {
    "@blueprintjs/core": "^4.2.1",
    "@blueprintjs/datetime": "^4.3.2",
    "@blueprintjs/select": "^4.8.17",
    "nanoid": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.6.0",
    "rxjs": "^7.8.0",
    "safety-match": "^0.4.4",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "styled-components": "^5.3.3",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:ci": "vitest run",
    "preview": "vite preview",
    "clear_build": "rm -rf ./dist",
    "deploy": "gh-pages --dist=./dist --branch=gh-pages --repo=git@github.com:dtgoitia/fitness-tracker.git",
    "clean_gh_pages_cache": "rm -rf ./node_modules/.cache/gh-pages",
    "deploy_from_local": "npm run clear_build && npm run build && npm run clean_gh_pages_cache && npm run deploy"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/styled-components": "^5.1.17",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^3.2.3",
    "prettier": "^2.5.1",
    "safe-types": "^4.27.0",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.5",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-pwa": "^0.17.3",
    "vitest": "^1.0.1"
  },
  "prettier": {
    "printWidth": 90
  }
}
