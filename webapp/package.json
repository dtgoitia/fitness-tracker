    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.3",
    "tailwindcss": "^3.0.2",
    "terser-webpack-plugin": "^5.2.5",
    "typescript": "^4.5.5",
    "web-vitals": "^0.2.4",
    "webpack": "^5.64.4",
